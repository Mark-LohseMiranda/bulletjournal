<div class="uk-margin uk-height-medium uk-width-large">
    <div class="uk-card uk-card-small uk-card-default uk-card-body">
        {{#each braindumps}}
        <form>
            <label for="braindumpTitle">Add a title here:</label><textarea class="uk-textarea uk-form-blank" rows="1" id="braindumpTitle" type="text">{{title}}</textarea>
            <label for="braindumpText"></label><textarea class="uk-textarea uk-form-blank" rows="5" id="braindumpText" placeholder="Dump your thoughts!">{{content}}</textarea>
        </form>
        {{/each}}
    </div>

</div>
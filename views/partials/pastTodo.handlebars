            {{#if (used content9)}}<li><label><input class="uk-checkbox" {{#if content9IsChecked}}checked{{/if}} type="checkbox" name="checkbox9" data-todoId={{id}} data-todoItem='content9IsChecked' id='checkbox9'> {{content9}}</label></li>{{/if}}
